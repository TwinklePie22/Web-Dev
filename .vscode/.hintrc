{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off",
        "nested-interactive": "off"
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "axe/name-role-value": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "axe/forms": "off",
    "axe/text-alternatives": "off",
    "highest-available-document-mode": "off",
    "meta-charset-utf-8": "off"
  }
}